    <defName>RC2_Caravan_Cabbage</defName>
    <label>cabbage merchant</label>
    <commonality>.25</commonality>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>
          <min>50</min>
          <max>100</max>
        </countRange>
      </li>
