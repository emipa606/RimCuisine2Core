  <RecipeDef ParentName="RC2_RecipeBase">
    <defName>RC2_ProcessCacao</defName>
    <label>make chocolate</label>
    <description>Combine cocoa beans and sweeteners to make delicous, delicious chocolate.</description>
    <jobString>Making Chocolate</jobString>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RC2_RawCocoaBeans</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>RC2_SweetsRaw</li>
            <li>RC2_SweetsProcessed</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <Chocolate>5</Chocolate>
    </products>
    <fixedIngredientFilter>
      <categories>
        <li>RC2_SweetsRaw</li>
        <li>RC2_SweetsProcessed</li>
      </categories>
      <disallowedThingDefs>
        <li>Chocolate</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>RC2_SweetsRaw</li>
        <li>RC2_SweetsProcessed</li>
      </categories>
      <disallowedThingDefs>
        <li>Chocolate</li>
      </disallowedThingDefs>
    </defaultIngredientFilter>
  </RecipeDef>
