<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--=========================== Basic Forestry ==============================-->
  <ThingDef ParentName="RC2_DeciduousTreeBase">
    <defName>RC2_PlantOrangeTree</defName>
    <label>citrus tree</label>
    <description>A fruit bearing tree that grows a number of orange citrus fruit. Bears fruit four times a year once it matures.  Only likes warmer climates.</description>
    <graphicData>
      <texPath>RC2_Things/RC2_Plants/OrangeTree</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <plant>
      <harvestYield>13</harvestYield>
      <fertilitySensitivity>0.2</fertilitySensitivity>
      <harvestAfterGrowth>0.75</harvestAfterGrowth>
      <harvestedThingDef>RC2_RawOrange</harvestedThingDef>
      <immatureGraphicPath>RC2_Things/RC2_Immature/OrangeTree</immatureGraphicPath>
      <sowResearchPrerequisites>
        <li>TreeSowing</li>
      </sowResearchPrerequisites>
      <sowMinSkill>3</sowMinSkill>
    </plant>
  </ThingDef>
